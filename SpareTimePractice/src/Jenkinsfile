pipeline{
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Example Build') {
           steps {
               bat 'echo start build'
               
           }
        }
        stage('Test') {
           steps {
               bat 'echo start test'
           }
        }
        stage('Deploy') {
           steps {
               bat 'echo start Deploy'               
           }
        }
    }
}
